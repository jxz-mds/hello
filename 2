int duplicateInArray(int *nums, int numsSize)
{
    int i, book[numsSize + 1];
     for (i = 1; i <= numsSize; i++)
    {
        book[i]=0;
    }
    for (i = 0; i < numsSize; i++)
    {
        book[nums[i]]++;
    }
    for (i = 1; i <=numsSize; i++)
    {
        if (book[i] > 1)
            return i;
    }
    return -1;
}
