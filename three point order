#include <stdio.h>
int main()
{
    int x1, y1, x2, y2, x3, y3;
    scanf("%d %d %d %d %d %d", &x1, &y1, &x2, &y2, &x3, &y3);
    int A = x2 - x1;
    int B = y2 - y1;
    int C = x3 - x1;
    int D = y3 - y1;
    if (A * D - B * C > 0)
        printf("0"); 
    else
        printf("1"); 
    return 0;
}
